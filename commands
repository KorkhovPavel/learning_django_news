django-admin startproject 'mysite' - создать проект
python manage.py runserver - запуск проекта
python manage.py startapp 'news' - создать приложение
python manage.py makemigrations - создать миграции
python manage.py sqlmigrate 'news 0001' - просмотр sql запроса
python manage.py migrate - запуск миграций